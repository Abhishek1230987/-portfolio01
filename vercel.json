{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_API_URL": "https://cyber-dev-console-backend.onrender.com"
  },
  "functions": {
    "app/api/**": {
      "runtime": "nodejs18.x"
    }
  }
}
